from itertools import permutations
for s,e,n,d,m,o,r,y in permutations(range(10),8):
    if s and m:
        send=s*1000+e*100+n*10+d
        more=m*1000+o*100+r*10+e
        money=m*10000+o*1000+n*100+e*10+y
        if send+more==money:
            print("send",send,"+ more",more,"money",money)
            break
else:
    print("noÂ yaar")
